{
    "extends": ["prettier", "eslint:recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "legacyDecorators": true
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true,
        "mocha": true
    },
    "rules": {
        "prefer-const": "error",
        "no-console": "off"
    }
}
